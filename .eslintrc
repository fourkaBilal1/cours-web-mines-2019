{
  "root": true,
  "parser": "babel-eslint",
  "plugins": ["html"],
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "lines-between-class-members": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  }
}
